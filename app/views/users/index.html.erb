<%= render partial: 'from', collection: @books, as: :book, locals: { user: @user, current_user: current_user } %>

<h2>Users</h2>

<% @user.each do |user| %>
  <div>
    <p>image：<%= image_tag user.get_profile_image(100,100) %></p>
    <p>name：<%= user.name %></p>

    <%= link_to "Show", user_path(user.id) %>
  </div>
<% end %>