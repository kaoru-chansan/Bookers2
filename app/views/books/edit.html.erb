<h2>Editing Book</h2>
<%= form_with model: book do |f| %>
  <label for="inputTitle">Title</label>
  <%= f.text_field :title, autofocus: true, id:"inputTitle", placeholder:"タイトル"%>
  <label for="inputBody">Opinion</label>
  <%= f.text_field :body, autofocus: true, id:"inputBody", placeholder:"感想"%>
  <%= f.submit "Update Book" %>
<% end %>

<%= link_to "Show", show_book_path %>
